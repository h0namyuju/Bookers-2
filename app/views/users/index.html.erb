<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Opinion</th>
    </tr>
  </thead>
  
  <tbody>
    <% @books.each do |book| %>
    <tr>
    <tb><%= link_to 'book.title', book_path(book.id) %></tb>
    <tb><%= book.body %></tb>
    </tr>
    <% end %>
  </tbody>
</table>



<%= render 'books/list', books: @books %>